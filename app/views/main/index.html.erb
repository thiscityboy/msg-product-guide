


 <div class="header animated slideInDown">
        <img src="<%= asset_path'logo.png' %>", width="60">
 </div>
 <div class="subhead animated slideInDown">
    Swipe through to view the collection
 </div>   
    <!-- Swiper -->
    <div class="swiper-container">
        <div class="parallax-bg">
            <%= image_tag "pattern.png", data: { swiper_parallax: "-33%" } %>
        </div>
        <div class="swiper-wrapper">
            <div class="swiper-slide">
                <div class="circle-one animated fadeIn" data-swiper-parallax="-100"><%= image_tag "circle1.png" %></div>
                <div class="circle-two animated fadeIn" data-swiper-parallax="-500"><%= image_tag "circle2.png" %></div>
                <div class="hat animated slideInUp" data-swiper-parallax="-700">
                    <%= image_tag "hat.png", width: "250", data: { swiper_parallax: "-1100" } %>
                </div>
            </div>
            <div class="swiper-slide">
                <div class="circle-one" data-swiper-parallax="-100"><%= image_tag "circle1.png" %></div>
                <div class="circle-two" data-swiper-parallax="-500"><%= image_tag "circle2.png" %></div>
                <div class="hoodie animated slideInUp" data-swiper-parallax="-700">
                    <%= image_tag "hoodies.png", width: "250", data: { swiper_parallax: "-1100" } %>
                </div>
            </div>
            <div class="swiper-slide">
                <div class="circle-one" data-swiper-parallax="-100"><%= image_tag "circle1.png" %></div>
                <div class="circle-two" data-swiper-parallax="-500"><%= image_tag "circle2.png" %></div>
                <div class="shoes animated slideInUp" data-swiper-parallax="-700">
                    <%= image_tag "shoes.png", width: "250", data: { swiper_parallax: "-1100" } %>
                </div>
            </div>
            <div class="swiper-slide">
                <div class="circle-one" data-swiper-parallax="-100"><%= image_tag "circle1.png" %></div>
                <div class="circle-two" data-swiper-parallax="-500"><%= image_tag "circle2.png" %></div>
                <div class="shirt animated slideInUp" data-swiper-parallax="-700">
                    <%= image_tag "t-shirts.png", width: "250", data: { swiper_parallax: "-1100" } %>
                </div>
            </div>
            <div class="swiper-slide">
                <div class="circle-one" data-swiper-parallax="-100"><%= image_tag "circle1.png" %></div>
                <div class="circle-two" data-swiper-parallax="-500"><%= image_tag "circle2.png" %></div>
                <div class="sunglasses animated slideInUp" data-swiper-parallax="-700">
                    <%= image_tag "sunglasses.png", width: "250", data: { swiper_parallax: "-1100" } %>
                </div>
            </div>
        </div>
    

    <div class="cta animated slideInUp">
        <a href="https://mp.vibescm.com/c/vcweyq" target="_parent"><img src="<%= asset_path'addtowallet.png' %>", width="220"></a>
    </div>
 
        <!-- Add Pagination -->
        <div class="swiper-pagination"></div>
    </div>

 <!-- Initialize Swiper -->
    <script>
    var swiper = new Swiper('.swiper-container', {
        paginationType: "progress",
        watchSlidesProgress: true,
        watchSlidesVisibility: true,
        preloadImages: true,
        centeredSlides: true,
        pagination: '.swiper-pagination',
        paginationClickable: true,
        spaceBetween: 340,
        parallax: true,
        speed: 400,
    });
    </script>
